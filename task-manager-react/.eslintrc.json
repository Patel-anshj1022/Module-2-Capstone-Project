{
  "extends": ["airbnb", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }]
  }
}